{
    "$schema": "http://json-schema.org/schema#",
    "title": "Carousel item",
    "description": "A carousel item component's schema definition.",
    "type": "object",
    "id": "carousel-item",
    "properties": {
        "id": {
            "title": "Unique ID",
            "type": "string"
        }
    },
    "reactProperties": {
        "children": {
            "title": "Building blocks",
            "type": "children",
            "defaults": [
                "text"
            ]
        }
    },
    "required": [
        "id"
    ]
}